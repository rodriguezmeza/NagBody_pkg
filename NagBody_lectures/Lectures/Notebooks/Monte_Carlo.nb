      RowBox[{"#", ",", 
       RowBox[{"simulation", "[", "#", "]"}]}], "}"}], "&"}], "/@", 
    RowBox[{"(", 
     RowBox[{"10", "^", 
      RowBox[{"{", 
       RowBox[{"1", ",", "2", ",", "3", ",", "4", ",", "5"}], "}"}]}], 
     ")"}]}], ",", 
   RowBox[{"TableHeadings", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"None", ",", 
      RowBox[{"{", 
       RowBox[{"\"\<n\>\"", ",", 
        RowBox[{"{", 
         RowBox[{"\[Pi]", ",", "\"\<error\>\""}], "}"}]}], "}"}]}], "}"}]}], 
