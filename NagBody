# ==================================================================
# CLASS
# ==================================================================
# class codes: [class code (with some modifications)]
#

MAKECLASS = Makefiles/class/NagBody_class

install_class:
    $(MAKE) -f $(MAKECLASS) install_class

clean_class:
    $(MAKE) -f $(MAKECLASS) clean_class

clean_class_dir:
    $(MAKE) -f $(MAKECLASS) clean_class_dir

packing_class:
    $(MAKE) -f $(MAKECLASS) packing_class

packing_gnu_class:
    $(MAKE) -f $(MAKECLASS) packing_gnu_class

packing_nagbody_gnu_class:
    $(MAKE) -f $(MAKECLASS) packing_nagbody_gnu_class

install_gnu_dirs_class:
    $(MAKE) -f $(MAKECLASS) install_gnu_dirs_class

install_nagbody_gnu_dirs_class:
    $(MAKE) -f $(MAKECLASS) install_nagbody_gnu_dirs_class

clean_gnu_class:
    $(MAKE) -f $(MAKECLASS) clean_gnu_class

clean_nagbody_gnu_class:
    $(MAKE) -f $(MAKECLASS) clean_nagbody_gnu_class


# ==================================================================
# hiclass code: [hi_class code (with some modifications)]
#

