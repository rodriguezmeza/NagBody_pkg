# ==================================================================
# ADDITIONAL TASKS ...
# ==================================================================
# Making additional directories:

INSTALLDIRS = bin man zip tests man/man1 local

install_dirs:
    -@for i in $(INSTALLDIRS) ; do \
        (if [ ! -d $$i ]; then \
            echo DIR $$i created; \
            mkdir $$i; \
        fi); done


