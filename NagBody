# ==================================================================
# psph code:

PSPH_DIR = NagBody_sources/gdgt/psph

install_psph: psph psph_man

psph:
	(cd $(PSPH_DIR);	$(MAKE) -f NagBody install)

psph_man:
	(cd $(PSPH_DIR);	$(MAKE) -f NagBody install_man)

clean_psph:
	(cd $(PSPH_DIR);	$(MAKE) -f NagBody clean)

clean_psph_dir:
	(cd $(PSPH_DIR);	$(MAKE) -f NagBody clean_dir)

packing_psph:
	(cd $(PSPH_DIR);	$(MAKE) -f NagBody packing_sources)






